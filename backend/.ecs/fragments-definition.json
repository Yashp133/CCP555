{
  "family": "fragments",
  "containerDefinitions": [
    {
      "name": "fragments",
      "image": "yashp133/fragments:latest",
      "cpu": 512,
      "memory": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": "yashkumar-fragments"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::844488432975:role/LabRole",
  "taskRoleArn": "arn:aws:iam::844488432975:role/LabRole"
}
